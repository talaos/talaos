    "karma": "^2.0.5",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^3.0.0",
    "protractor": "^5.1.1",
    "ts-loader": "^3.5.0",
    "ts-node": "^3.3.0",
    "typescript": "^2.9.2"
  },
  "description": "An IT management UI for GLPI, monitoring...",
  "config": {
