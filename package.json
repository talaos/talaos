    "cordova-plugin-splashscreen": "^4.1.0",
    "cordova-plugin-whitelist": "^1.3.3",
    "core-js": "^2.4.0",
    "ionic-angular": "3.9.2",
    "ionic-plugin-keyboard": "^2.2.1",
    "ionicons": "3.0.0",
    "lodash": "^4.17.10",
    "rxjs": "^5.5.11",
    "sw-toolbox": "3.6.0",
    "zone.js": "^0.8.26"
