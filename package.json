    "@fortawesome/fontawesome-free": "^5.2.0",
    "@ionic-native/core": "^4.11.0",
    "@ionic-native/splash-screen": "^4.4.0",
    "@ionic-native/status-bar": "^4.4.0",
    "@ionic/storage": "2.1.3",
